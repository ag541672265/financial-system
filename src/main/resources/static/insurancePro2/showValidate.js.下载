//关闭弹出层
function closeWindow(dom){
	   $('#'+dom).hide();
	   $(".mask,.tt").remove();
	   $("body").find("select").show();
	   if($("#select_102").length>0){
		   $("#select_102").hide();
	   }
	   if($("#Jxianqu").length>0){
		   if($("#count").val()==1 || $("#count").val()==2){
				$("#Jxianqu").hide();
			}
	   }
	   
	}
